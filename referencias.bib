@Article{doNascimento2015304,
  Title                    = {On the use of multiple heterogeneous devices to speedup the execution of a computational model of the Human Immune System },
  Author                   = {T.M. do Nascimento and J.M. de Oliveira and M.P. Xavier and A.B. Pigozzo and R.W. dos Santos and M. Lobosco},
