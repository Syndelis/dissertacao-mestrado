@Article{Hillen2001,
  author    = {Hillen, T. and Painter, K.},
  title     = {Global Existence for a Parabolic Chemotaxis Model with Prevention of Overcrowding},
  journal   = {Advances in Applied Mathematics},
  year      = {2001},
